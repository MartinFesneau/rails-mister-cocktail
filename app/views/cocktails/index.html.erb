<%= render 'layouts/shared/banner' %>
<div class="container">
  <div class="wrapper" id="cocktails-list">
      <% @cocktails.each do |cocktail| %>
        <div class="card"d ata-aos="fade-in" style="background-image: url(<%= cl_image_path cocktail.photo.key, height: 300, width: 150, crop: :fill %>)">
          <div>
            <%= link_to cocktail.name, cocktail_path(cocktail.id), class:"card-name" %>
          </div>
        </div>
      <% end %>
  </div>
</div> 

